{
  "name": "dart-completer",
  "version": "1.1.1",
  "description": "A Typescript port of Dart's Completer.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "npx cross-env NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges && make-coverage-badge",
    "prepublish": "npx tsc"
  },
  "exports": "./dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ciriousjoker/dart-completer.git"
  },
  "keywords": [
    "typescript",
    "dart",
    "completer",
    "promise",
    "future",
    "async",
    "await"
  ],
  "author": "Philipp Bauer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ciriousjoker/dart-completer/issues"
  },
  "homepage": "https://github.com/ciriousjoker/dart-completer#readme",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "cross-env": "^7.0.3",
    "jest": "^27.5.1",
    "jest-coverage-badges": "^1.1.2",
    "make-coverage-badge": "^1.2.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  }
}
